<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Url:</strong>
    <%= post.url %>
  </p>

  <p>
    <strong>Summary:</strong>
    <%= post.summary %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= post.body %>
  </p>

  <p>
    <strong>Image:</strong>
    <%= post.image.attached? %>
  </p>

  <p>
    <strong>Account:</strong>
    <%= post.account.first_name + " " + post.account.last_name %>
  </p>

  <div> 
    <img src="<%= post_main(post) %>" alt="" width="500px">
  </div>
</div>
